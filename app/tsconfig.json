{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "es2022"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@swapback/sdk": ["./src/sdk/index.ts"],
      "rpc-websockets/dist/lib/client": ["./src/lib/rpc-websockets-shim.js"],
      "rpc-websockets/dist/lib/client/websocket.browser": ["./src/lib/rpc-websockets-browser-shim.js"],
      "rpc-websockets/dist/lib/client/websocket.browser.cjs": ["./src/lib/rpc-websockets-browser-shim.js"]
    },
    "target": "es2020"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
